>>> make vrt export
Info: No sentence information found in corpus
mkdir -p annotations/ -m 775
python2 -m sb.fileid --out annotations/fileids --files "corpus" --prefix ""
mkdir -p annotations/ -m 775
python2 -m sb.xmlparser --source original/corpus.xml --text annotations/corpus.@TEXT --skip "" --elements "text" --annotations "annotations/corpus.text" --overlap "" --fileid "corpus" --fileids annotations/fileids --header "" --headers "" --header_annotations "" --skip_if_empty "" --skip_entities "" --autoclose "" --allow_xml_chars ""
python2 -m sb.segment --element p --out annotations/corpus.paragraph --text annotations/corpus.@TEXT --chunk annotations/corpus.text --segmenter blanklines --model ""
python2 -m sb.segment --element s --out annotations/corpus.sentence --text annotations/corpus.@TEXT --chunk annotations/corpus.paragraph --segmenter punkt_sentence --model "/home/dan/code/annotate/models/punkt-nltk-svenska.pickle"  
python2 -m sb.segment --element w --out annotations/corpus.token --text annotations/corpus.@TEXT --chunk annotations/corpus.sentence --segmenter better_word --model "/home/dan/code/annotate/models/bettertokenizer.sv" --no_pickled_model=true
python2 -m sb.parent --parents --out annotations/corpus.parents.token.text --text annotations/corpus.@TEXT --parent annotations/corpus.text --child annotations/corpus.token --ignore_missing_parent ""
python2 -m sb.annotate --text_spans --out annotations/corpus.token.word --text annotations/corpus.@TEXT --chunk annotations/corpus.token
python2 -m sb.parent --children --out annotations/corpus.children.sentence.token --text annotations/corpus.@TEXT --parent annotations/corpus.sentence --child annotations/corpus.token --ignore_missing_parent ""
python2 -m sb.hunpos --out annotations/corpus.token.msd --word annotations/corpus.token.word --sentence annotations/corpus.children.sentence.token --model /home/dan/code/annotate/models/hunpos.suc3.suc-tags.default-setting.utf8.model --tag_mapping "" --morphtable "/home/dan/code/annotate/models/hunpos.saldo.suc-tags.morphtable" --patterns "/home/dan/code/annotate/models/hunpos.suc.patterns"
python2 -m sb.annotate --select --out annotations/corpus.token.pos --annotation annotations/corpus.token.msd --index 0 --separator "."
python2 -m sb.number --relative --out annotations/corpus.token.ref --parent_children annotations/corpus.children.sentence.token
python2 -m sb.saldo --out "annotations/corpus.token.baseform annotations/corpus.token.lemgram annotations/corpus.token.saldo" --annotations "gf lem saldo" \
	--word annotations/corpus.token.word --msd annotations/corpus.token.msd --models /home/dan/code/annotate/models/saldo.pickle --delimiter "|" --affix="|" --precision="" --min_precision=0.66 --precision_filter "max" --sentence annotations/corpus.children.sentence.token --reference annotations/corpus.token.ref --skip_multiword ""
python2 -m sb.compound_simple --out_prefix annotations/corpus.token.prefix --out_suffix annotations/corpus.token.suffix --word annotations/corpus.token.word --msd annotations/corpus.token.msd --model /home/dan/code/annotate/models/saldo.compound.pickle
python2 -m sb.malt --maltjar maltparser-1.7.2/maltparser-1.7.2.jar --out annotations/corpus.token.malt --sentence annotations/corpus.children.sentence.token --word annotations/corpus.token.word --msd annotations/corpus.token.msd --pos annotations/corpus.token.pos --model /home/dan/code/annotate/models/swemalt-1.7.2.mco
python2 -m sb.annotate --select --out annotations/corpus.token.dephead --annotation annotations/corpus.token.malt --index 1
python2 -m sb.annotate --chain --out annotations/corpus.token.dephead.ref --annotations "annotations/corpus.token.dephead annotations/corpus.token.ref"
python2 -m sb.annotate --select --out annotations/corpus.token.deprel --annotation annotations/corpus.token.malt --index 0
python2 -m sb.number --position --out annotations/corpus.token.n --texts annotations/corpus.@TEXT --chunks annotations/corpus.token
python2 -m sb.cwb --export --format vrt --out annotations/corpus.vrt \
	--order annotations/corpus.token.n \
	--annotations_columns "annotations/corpus.token.word annotations/corpus.token.pos annotations/corpus.token.msd annotations/corpus.token.baseform annotations/corpus.token.lemgram annotations/corpus.token.saldo annotations/corpus.token.prefix annotations/corpus.token.suffix annotations/corpus.token.ref annotations/corpus.token.dephead.ref annotations/corpus.token.deprel" \
	--annotations_structs "annotations/corpus.text:annotations/corpus.parents.token.text" \
	--columns "word pos msd lemma    lex     saldo prefix suffix ref dephead     deprel" \
	--structs "text"
mkdir -p export.original/ -m 775
python2 -m sb.cwb --export --format xml --out export.original/corpus.xml \
	--order annotations/corpus.token.n \
	--annotations_columns "annotations/corpus.token.word annotations/corpus.token.pos annotations/corpus.token.msd annotations/corpus.token.baseform annotations/corpus.token.lemgram annotations/corpus.token.saldo annotations/corpus.token.prefix annotations/corpus.token.suffix annotations/corpus.token.ref annotations/corpus.token.dephead.ref annotations/corpus.token.deprel" \
	--annotations_structs "annotations/corpus.text:annotations/corpus.parents.token.text" \
	--columns "word pos msd lemma    lex     saldo prefix suffix ref dephead     deprel" \
	--structs "text" \
	--fileid "corpus" --fileids annotations/fileids
